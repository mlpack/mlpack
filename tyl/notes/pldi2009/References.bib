nt setpoints, indirectly lead to near-optimal operation with acceptable loss. This is denoted 'self-optimizing' control. (C) 2000 Elsevier Science Ltd. All rights reserved.}, Keywords = {chemical process control; control structure design; plantwide controlcontrol-system; variables; design},   Year = {2000} }@book{Slotine1991,   Author = {Slotine, J. J. E. and Li, Weiping},   Title = {Applied nonlinear control},   Publisher = {Prentice Hall},   Address = {Englewood Cliffs, N.J.}, Keywords = {Nonlinear control theory.Commande non linaire.Automatic control},   Year = {1991} }@book{Smith1987,   Author = {Smith, J. M. and Van Ness, H. C.},   Title = {Introduction to chemical engineering thermodynamics},   Publisher = {McGraw-Hill},   Address = {New York},   Edition = {4th},   Series = {McGraw-Hill chemical engineering series;}, Keywords = {Thermodynamics.Chemical engineering.Chemical engineering; Thermodynamics},   Year = {1987} }@book{Smith1997,   Author = {Smith, Carlos A. and Corripio, Armando B.},   Title = {Principles and practice of automatic process control},   Publisher = {J. Wiley},   Address = {New York},   Edition = {2nd}, Keywords = {Chemical process control.},   Year = {1997} }@article{Snyder2003,   Author = {Snyder, M. and Gerstein, M.},   Title = {Genomics - Defining genes in the genomics era},   Journal = {Science},   Volume = {300},   Number = {5617},   Pages = {258--260}, Keywords = {protein-coding genes; saccharomyces-cerevisiae genome; yeast genome;total number; pseudogenes; evolution; dna},   Year = {2003} }@article{Sodhi2001,   Author = {Sodhi, M. S.},   Title = {Applications and opportunities for operations research in Internet-enabled supply chains and electronic marketplaces},   Journal = {Interfaces},   Volume = {31},   Number = {2},   Pages = {56--69},   Abstract = {The Internet is creating opportunities to apply optimization. Firms can use operations research (OR) to improve 