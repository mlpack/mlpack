        <type><ref refid="namespacemlpack_1_1hpt_1a7146c132523ba701e4baee3e1cad424d" kindref="member">TupleOfHyperParameters</ref>&lt; Args... &gt;</type>
        <definition>TupleOfHyperParameters&lt;Args...&gt; Optimize</definition>
